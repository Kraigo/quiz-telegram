<div class="container">
    <div class="card">
        <div class="card-block">
            <div class="card-title">Questions ({{questions.length}})</div>
            <ul class="list-group">
                {{#each questions}}                
                    <li class="list-group-item">
                        {{this.title}}
                        <br />
                        <small>{{this.answer}}</small>
                    </li>
                {{/each}}
            </ul>
        </div>
    </div>
</div>